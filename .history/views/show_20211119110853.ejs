<!DOCTYPE html>
<!-- Created By CodingNepal -->
<html lang="en" dir="ltr">
   <head>
      <meta charset="utf-8">
      <title>Awesome Product Card Design | CodingNepal</title>
      <link type="text/css" href="/css/products.css" rel="stylesheet">
      <link rel="stylesheet" href="/css/styles.css">
      <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
      <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
   </head>
   
   <body>
      <header>
         <div class="topnav">
               <a href="/">Home</a>
               <a href="/show-all/">Products</a>
         </div>
      </header>
      <table class="table table-dark">
         <thead>
           <tr>
             <th scope="col">#</th>
             <th scope="col">Product Name</th>
             <th scope="col">Product Category</th>
             <th scope="col">Product Price</th>
             <th scope="col">Product Description</th>
           </tr>
         </thead>
         <tbody>
            <% prods.forEach(function(prod) { %>
               <tr>
                  <th scope="row">{{prod.prodId}}</th>
                  <td>prod.prodName</td>
                  <td>prod.prodCategory</td>
                  <td>prod.prodPrice</td>
                  <td>prod.prodDescription</td>
               </tr>
            <% }); %>
         </tbody>
      </table>
   </body>
</html>